<table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">#</th>
        <th scope="col">First name</th>
        <th scope="col">Last mane</th>
        <th scope="col">Email</th>   
        {{#resultview}}    
        <th scope="col">Status</th> 
        <th scope="col">Time</th> 
        {{/resultview}}    
      </tr>
    </thead>
    <tbody>
        {{#data}}
      <tr>
        <th scope="row">{{index}}</th>
        <td>{{firstname}}</td>
        <td>{{lastname}}</td>
        <td>{{email}}</td>
        {{#resultview}}  
             <td>{{status}}</td>
             <td>{{time}}</td>
        {{/resultview}}  
      </tr>
      {{/data}}    
    </tbody>
  </table>